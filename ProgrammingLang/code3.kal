

def isPrime(num)
    
    div = 3
    prime = 1

    if (num % 2) = 0
        return 0

    while div < num
        if (num % div) = 0
            prime = 0
        div = div + 2
    return prime

def primes(total)    
    i = 1
    while i < total
        if (isPrime(i))
            say "PRIME NUMBER: " (i)
        i = i + 1

say (primes(10000))